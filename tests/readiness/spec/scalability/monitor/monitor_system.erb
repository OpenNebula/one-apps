HOSTNAME   = host-<%=@id%>
ARCH       = x86_64
MODELNAME  = "Intel(R) Core(TM) i5-6200U CPU @ 2.30GHz"
HYPERVISOR = dummy
CPUSPEED   = 2400
<%# ------------------------------------------------------------------------ -%>
<%# MEMORY                                                                   -%>
<%# ------------------------------------------------------------------------ -%>
<% tmem=7881064 -%>
TOTALMEMORY =<%= tmem %>
<%# ------------------------------------------------------------------------ -%>
<%# CPU                                                                      -%>
<%# ------------------------------------------------------------------------ -%>
<% tcpu=400 -%>
TOTALCPU =<%= tcpu %>
<%# ------------------------------------------------------------------------ -%>
<%# DATASTORES                                                               -%>
<%# ------------------------------------------------------------------------ -%>
<% tdisk=512000 -%>
<% udisk=tdisk-rand(512000) -%>
<% fdisk=tdisk-udisk -%>
DS_LOCATION_TOTAL_MB =<%= tdisk %>
DS_LOCATION_FREE_MB  =<%= fdisk %>
DS_LOCATION_USED_MB  =<%= udisk %>
<% [0,1,2].each { |i| -%>
DS = [
    ID = <%= i %>,
    TOTAL_MB =<%= tdisk %>,
    FREE_MB  =<%= fdisk %>,
    USED_MB  =<%= udisk %>
]
<% } -%>