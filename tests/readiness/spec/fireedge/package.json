{
  "name": "FireEdge-Tests",
  "version": "1.0.0",
  "description": "Opennebula FireEdge test",
  "main": "src/index.js",
  "scripts": {
    "cypress:install": "./node_modules/.bin/cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "rimraf cypress/results/* && node command.js",
    "cypress:run-kvm": "rimraf cypress/results/* && NO_REPORT=true ENV_TYPE=kvm node command.js",
    "cypress:run-apache": "rimraf cypress/results/* && NO_REPORT=true ENV_TYPE='apache|func' node command.js",
    "cypress:run-firefox": "rimraf cypress/results/* && node command.js browser=firefox",
    "lint": "eslint cypress/",
    "lint:fix": "eslint --fix cypress/"
  },
  "author": "opennebula.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16.20.2"
  },
  "keywords": [
    "test",
    "cypress",
    "fireedge",
    "opennebula"
  ],
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-opennebula": "2.0.2",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-mocha": "8.2.0"
  },
  "dependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.18.2",
    "@cypress/webpack-preprocessor": "5.12.0",
    "@nuintun/qrcode": "3.3.0",
    "babel-loader": "8.2.5",
    "cypress": "12.17.0",
    "cypress-file-upload": "5.0.8",
    "cypress-real-events": "1.11.0",
    "cypress-wait-until": "1.7.2",
    "dotenv": "14.1.0",
    "fast-xml-parser": "4.0.8",
    "fs-extra": "10.0.0",
    "luxon": "3.3.0",
    "md5": "2.3.0",
    "mochawesome": "7.0.1",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "6.0.1",
    "otpauth": "7.1.3",
    "path": "0.12.7",
    "pixelmatch": "5.2.1",
    "pngjs": "6.0.0",
    "puppeteer": "10.2.0",
    "rimraf": "3.0.2",
    "uuid": "8.3.2",
    "webpack": "5.73.0",
    "yaml": "1.10.0"
  }
}
